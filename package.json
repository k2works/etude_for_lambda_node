{
  "name": "sam-scaffold",
  "version": "1.0.0",
  "description": "Example project description.",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf ./dist",
    "lint": "eslint \"src/**/*.js\"",
    "test": "mocha --recursive --throw-deprecation --require babel-core/register --require babel-polyfill \"src/**/*.test.js\"",
    "precommit": "npm run lint & npm run test",
    "prepush": "npm run build",
    "start": "babel-node src/server",
    "dev:start": "nodemon --ignore dist/lib --exec babel-node src/server",
    "prod:build": "rimraf dist/app && babel src -d dist/app --ignore lambdas,.test.js",
    "prod:start": "cross-env NODE_ENV=production pm2 start dist/app/server && pm2 logs",
    "prod:stop": "pm2 delete server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giftbit/sam-scaffold.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Giftbit/sam-scaffold/issues"
  },
  "homepage": "https://github.com/Giftbit/sam-scaffold#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.0",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "json-loader": "^0.5.7",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.4",
    "pm2": "^2.10.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "zip-webpack-plugin": "^3.0.0"
  },
  "dependencies": {
    "cassava": "^2.0.1",
    "compression": "^1.7.2",
    "express": "^4.16.3"
  }
}
